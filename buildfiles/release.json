{
    "_basic": {
        "productCode": "brithon-crm",
        "packageName": "brithon-crm",
        "productName": "Brithon CRM",
        "frameworkVersion":  "2.0",
        "templates": [
            "brithon-crm.php"
        ],
        "wpVersionRequires": "3.7",
        "wpVersionTested": "4.2",
        "updaterApiUrl": "http://www.brithon.com/"
    },

    "editions": {
        "developer": {
            "addons": [
                "subscriptions", "registration", "common", "todomvc", "sso"
            ],
            "libs": {
                "php": [
                    "index.php",
                    "composer.json",
                    "composer.lock",
                    "vendor/autoload.php",
                    "vendor/composer",
                    "vendor/stripe",
                    "vendor/ccampbell"
                ]
            }
        },

        "multisite": {
            "addons": [
                "subscriptions", "registration", "common", "sso"
            ]
        }
    }
}
